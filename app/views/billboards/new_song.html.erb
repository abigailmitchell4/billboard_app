<h1 class="ui header">Add A Song To: '<%= @billboard.name %>'</h1>	
<div class="container">
  <div class="ui card">
  <% if @songs.any? %>
    <% @songs.each do |song| %>
      <%= link_to song.title, add_billboard_song_path(@billboard, song), method: :post %>
    <% end %>
     <% else %>
    <div>
  <h2>No Songs On Billboard</h2>
  </div>
  <% end %>
  </div>
</div>
</br>
<%= link_to 'Back', billboards_path, class: 'ui button' %>